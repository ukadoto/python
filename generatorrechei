import random
rech = {
    1:{1:'Не вызывает сомнений тот факт, что ', 2:'С другой стороны', 3:'Равным образом', 4:'Не следует однако забывать, что', 5:'Таким образом', 6:'Повседневная практика показывает, что'},
    2:{1:'реализация намеченных плановых заданий', 2:'рамки и место обучения кадров', 3:'постоянный количественный рост и сфера нашей активности', 4:'сложившаяся структура организации', 5:'новая модель организационной деятельности', 6:'дальнейшее развитие различных форм деятельности'},
    3:{1:'играет важную роль в формировании', 2:'требуют от нас анализа', 3:'требуют определения и уточнения', 4:'способствуют подготовке и реализации', 5:'обеспечивает широкому кругу специалистов участие в формировании', 6:'позволяет выполнять важные задания по разработке'},
    4:{1:'существенных финансовых и административных условий. ', 2:'жальнейших направлений развития. ', 3:'системы массового участия. ', 4:'позиций занимаемых участниками в отношении поставленных задач. ', 5:'новых предложений. ', 6:'направлений прогрессивного развития. '}

}

z = int(input())
#z = int(z)
e = 1

while e <= z:
    e = e + 1
    q = 0
    while q < len(rech):
        q = q + 1 
        y = random.randint(1,6)
        print(rech[q][y], end=' ')
